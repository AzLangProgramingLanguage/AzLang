dəyişən a = 5
funksiya artir(dəyişən c: ədəd)
    c = c + 1
    əgər c == 18
        çap("Artıq böyüksən!")
        qaytar c
    yoxsa c == 17
        çap("Bir yaşın qalıb böyüməyə.")
        qaytar c
    əks
        qaytar c
sabit b = artir(a)  // Hər ikisidə eyni nəticə verir
çap(c)              // 

sabit a = a



